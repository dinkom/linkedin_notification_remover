{
  "manifest_version": 2,

  "name": "LinkedIn Notification Remover",
  "description": "This extension will remove the 'We found new people in your network' notification from LinkedIn",
  "version": "1.0.1",

  "content_scripts": [ {
        "js": [ "linover.js" ],
        "matches": [ "http://*/*", "https://*/*" ]
    } ],

  "permissions": [
    "http://*/*", "https://*/*", "tabs"
  ],

  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" 
  }
}